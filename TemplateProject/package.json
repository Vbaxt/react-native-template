{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"tsc --watch\" \"node node_modules/react-native/local-cli/cli.js start\"",
    "lint": "eslint app",
    "test": "jest",
    "build": "tsc"
  },
  "dependencies": {
    "react": "16.0.0-alpha.6",
    "react-native": "0.43.3",
    "react-native-config": "^0.3.1",
    "react-navigation": "1.0.0-beta.8",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/react": "^15.0.22",
    "@types/react-native": "^0.43.6",
    "@types/react-redux": "^4.4.39",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "1.9.1",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-native": "^2.3.1",
    "jest": "^19.0.2",
    "react-test-renderer": "~15.4.2",
    "redux-mock-store": "^1.2.2",
    "remote-redux-devtools": "^0.5.7",
    "ts-jest": "^19.0.14",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(j|t)sx?$",
    "testPathIgnorePatterns": ["/node_modules/", "/build"],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-clone-referenced-element|expo|@expo/*|exponent|@exponent/*)"
    ],
    "setupFiles": ["./jest-setup.js"]
  }
}
